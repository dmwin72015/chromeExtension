{
  "name": "Download Selected Links",
  "description": "Select links on a page and download them.",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches": ["http://www.hewebgl.com/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}